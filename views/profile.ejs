              <div class="one-one">
                  <h6> YOUR INTERESTS </h6>
                  <ul>
                    <% for (let i = 0; i < currentUser.interests.length; i++) { %>
                      <li><%= currentUser.interests[i]  %></li>
                      <!-- <li>UGC And Micro-Influence Marketing</li>
                      <li>Live-Stream Shopping</li>
                      <li>Coding</li>
                      <li>Video Production</li>
